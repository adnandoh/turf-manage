# ===============================================
# ADMIN FRONTEND - PRODUCTION ENVIRONMENT
# ===============================================

# Application Info
VITE_APP_NAME=TurfBook Admin
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Sports Turf Booking System - Admin Management Portal

# Production API Configuration
VITE_API_BASE_URL=https://turf-backend-production.up.railway.app
VITE_API_TIMEOUT=20000

# Backend API Endpoints (Production)
VITE_CRICKET_API_URL=https://turf-backend-production.up.railway.app/api/cricket
VITE_PICKLEBALL_API_URL=https://turf-backend-production.up.railway.app/api/pickleball
VITE_ADMIN_API_URL=https://turf-backend-production.up.railway.app/api/admin
VITE_AUTH_API_URL=https://turf-backend-production.up.railway.app/api-token-auth
VITE_DASHBOARD_API_URL=https://turf-backend-production.up.railway.app/api/admin/dashboard

# Production Settings
VITE_NODE_ENV=production
VITE_DEBUG=false
VITE_LOG_LEVEL=error
VITE_ENABLE_REDUX_DEVTOOLS=false

# Security Settings (Production)
VITE_ENABLE_SESSION_TIMEOUT=true
VITE_SESSION_WARNING_MINUTES=5
VITE_MAX_LOGIN_ATTEMPTS=3
VITE_LOCKOUT_DURATION_MINUTES=30

# Production URLs
VITE_SITE_URL=https://turf-manage.vercel.app
VITE_CDN_URL=https://turf-manage.vercel.app
VITE_ASSETS_CDN_URL=https://turf-manage.vercel.app

# Analytics (Production)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_HOTJAR_ID=123456
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Error Tracking (Production)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_BUGSNAG_API_KEY=your-bugsnag-api-key
VITE_ENABLE_ERROR_BOUNDARY=true

# Monitoring & Logging (Production)
VITE_ENABLE_API_LOGGING=true
VITE_ENABLE_USER_ACTION_LOGGING=true
VITE_LOG_RETENTION_DAYS=90

# Integration Settings (Production)
VITE_SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
VITE_DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/WEBHOOK

# Build Settings (Production)
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true